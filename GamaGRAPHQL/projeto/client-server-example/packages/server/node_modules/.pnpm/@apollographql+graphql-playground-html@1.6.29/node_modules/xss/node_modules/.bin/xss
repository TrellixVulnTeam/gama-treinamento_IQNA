#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/client-server-example/packages/server/node_modules/.pnpm/xss@1.0.9/node_modules/xss/bin/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/client-server-example/packages/server/node_modules/.pnpm/xss@1.0.9/node_modules/xss/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/client-server-example/packages/server/node_modules/.pnpm/xss@1.0.9/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/client-server-example/packages/server/node_modules/.pnpm/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/client-server-example/packages/server/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/client-server-example/packages/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/client-server-example/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/node_modules:/mnt/c/Users/jmc_c/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/client-server-example/packages/server/node_modules/.pnpm/xss@1.0.9/node_modules/xss/bin/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/client-server-example/packages/server/node_modules/.pnpm/xss@1.0.9/node_modules/xss/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/client-server-example/packages/server/node_modules/.pnpm/xss@1.0.9/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/client-server-example/packages/server/node_modules/.pnpm/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/client-server-example/packages/server/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/client-server-example/packages/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/client-server-example/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/projeto/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/GamaGRAPHQL/node_modules:/mnt/c/Users/jmc_c/GAMAsoftware/node_modules:/mnt/c/Users/jmc_c/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/xss" "$@"
else
  exec node  "$basedir/../../bin/xss" "$@"
fi
